
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>display</title>
</head>
<body>
    <span style="display:none;">
        <img id="map" onload="drawImage" src="~/Images/map.png" alt="The Map">
    </span>
    
    <div>

        <script>
            var img = document.getElementById("map");
            var imgs = new Image();
            imgs.src = './Images/map.png';
            var c = document.createElement("canvas");
            document.body.appendChild(c);
            c.width = img.width;
            c.height = img.height;
            var ctx = c.getContext("2d");
            ctx.lineWidth = 10;
            ctx.strokeStyle = '#00ff00';
            ctx.clearRect(0, 0, c.width, c.height);
            function drawImage() {
                ctx.drawImage(img, 0, 0);
            }
            function loadCanvas() {
                drawImage();
                ctx.beginPath();
                ctx.moveTo(0, 0);
                ctx.lineTo(200, 100);
                ctx.moveTo(200, 100);
                ctx.lineTo(400, 400);
                ctx.moveTo(400, 400);
                ctx.lineTo(900, 1200);
                ctx.moveTo(900, 1200);
                ctx.lineTo(2000, 1400);
                ctx.stroke();
            }
            loadCanvas();

        </script>
    </div>
</body>
</html>
